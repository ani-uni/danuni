{
  "private": true,
  "packageManager": "pnpm@9.11.0",
  "scripts": {
    "prepare": "husky",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint",
    "commitlint": "commitlint --edit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "pnpm dlx commitlint --edit "
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --ignore-path ./.prettierignore --write "
    ]
  },
  "author": "rinne",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ani-uni/dan-uni.git"
  },
  "bugs": {
    "url": "https://github.com/ani-uni/dan-uni/issues"
  },
  "homepage": "https://github.com/ani-uni/dan-uni",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@innei/eslint-config-ts": "^0.15.0",
    "@innei/prettier": "^0.15.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@sxzz/eslint-config": "^4.3.0",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^2.1.1",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "rollup": "^4.23.0",
    "tslib": "^2.7.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^7.18.0",
    "vitest": "^2.1.1"
  }
}